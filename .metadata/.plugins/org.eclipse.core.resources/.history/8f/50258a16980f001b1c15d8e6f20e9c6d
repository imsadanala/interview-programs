 */
package com.interview.programs;

/**
 * @author Suresh Sadanala
 *
 */
public class ReverseNumber {
	public static void main(String[] args) {
		
		int orgNum = 12345, revNum,remainder =0;
		while() {
			remainder = orgNum%10;
			revNum = revNum*10+remainder;
			orgNum = orgNum/10;
			
		}
		
	}
}
